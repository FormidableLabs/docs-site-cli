    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-ga": "^2.1.2",
    "react-router": "^2.8.1",
    "react-router-scroll": "^0.3.2",
    "static-site-generator-webpack-plugin": "^3.0.0",
    "style-loader": "^0.13.1",
    "typography": "^0.13.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "devDependencies": {
    "eslint": "^2.10.2",
    "eslint-config-formidable": "^1.0.1",
    "eslint-plugin-filenames": "^1.1.0",
