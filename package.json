    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "color": "^0.11.3",
    "commander": "^2.9.0",
    "cross-spawn": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.30.0",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.4.0",
    "history": "^2.1.0",
    "image-webpack-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "markdown-it": "^8.0.0",
