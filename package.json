{
  "name": "docs-site-cli",
  "version": "0.0.0",
  "bin": "./bin/docs-site-cli.js",
  "description": "A CLI tool to generate a static documentation site for your project",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config config/webpack.config.build.js -p --progress --bail",
    "dev": "webpack-dev-server --port 3000 --config config/webpack.config.dev.js --history-api-fallback",
    "lint": "eslint --ext .js bin lib config template",
    "test": "npm run lint"
  },
  "author": "Kyle Cesmat <kyle.cesmat@formidable.com>",
  "repository": "https://github.com/FormidableLabs/docs-site-cli",
  "license": "MIT",
